package Computers;

public class ComputerFunctions{
     
    private String mouse;
    private double cost;
    private double time;
    private String laptop;
    private int size;
    private double awake;
    private String cpu;
    private String disk;
    private String mb;
    private String country;
    private double countryElectricityAverage;
    private double electricityUse;
    private double totalCost;
    private double totalElectricityCost;
    private double totalBatteryCost;
    
    
    public double getTotalBatteryCost() {
        return (time);
    }



    public void setTotalBatteryCost(double totalBatteryCost) {
        this.totalBatteryCost = totalBatteryCost;
    }



    private double totalComputerCost;
    private double mice;
    
    public double getElectricityUse() {
        return electricityUse;
    }



    public void setElectricityUse(double electricityUse) {
        this.electricityUse = electricityUse;
    }



    public String getLaptop() {
        return laptop;
    }



    public void setLaptop(String laptop) {
        this.laptop = laptop;
    }



    public int getSize() {
        return size;
    }



    public void setSize(int size) {
        this.size = size;
    }



    public double getTime() {
        return time;
    }



    public void setTime(double time) {
        this.time = time;
    }



    public ComputerFunctions(){
        
    }



    public double getCost() {
        return cost;
    }



    public void setCost(double cost) {
        this.cost = cost;
    }



    public String getMouse() {
        return mouse;
    }



    public void setMouse(String mouse) {
        this.mouse = mouse;
    }



    public String getCountry() {
        return country;
    }



    public void setCountry(String country) {
        this.country = country;
    }



    public double getCountryElectricityAverage() {
        
        return countryElectricityAverage;
    }



    public void setCountryElectricityAverage(double countryElectricityAverage) {
        this.countryElectricityAverage = countryElectricityAverage;
    }



    public double getTotalCost() {
        return (totalBatteryCost + totalElectricityCost + totalComputerCost);
    }



    public void setTotalCost(double totalCost) {
        this.totalCost = totalCost;
    }



    

    public double getTotalElectricityCost() {
        return totalElectricityCost;
    }



    public void setTotalElectricityCost(double totalElectricityCost) {
        this.totalElectricityCost = totalElectricityCost;
    }



    public double getTotalComputerCost() {
        return totalComputerCost;
    }



    public void setTotalComputerCost(double totalComputerCost) {
        this.totalComputerCost = totalComputerCost;
    }



    public double getMice() {
        return mice;
    }



    public void setMice(double mice) {
        this.mice = mice;
    }



    public String getCpu() {
        return cpu;
    }



    public void setCpu(String cpu) {
        this.cpu = cpu;
    }



    public String getDisk() {
        return disk;
    }



    public void setDisk(String disk) {
        this.disk = disk;
    }



    public String getMb() {
        return mb;
    }



    public void setMb(String mb) {
        this.mb = mb;
    }



    public double getAwake() {
        return awake;
    }



    public void setAwake(double awake) {
        this.awake = awake;
    }
    

}
